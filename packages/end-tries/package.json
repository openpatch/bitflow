{
  "name": "@bitflow/end-tries",
  "version": "0.4.5",
  "author": "Mike Barkmin",
  "homepage": "https://github.com/openpatch/bitflow#readme",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.mjs",
  "types": "dist/index.d.ts",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openpatch/bitflow.git",
    "directory": "packages/end-tries"
  },
  "bugs": {
    "url": "https://github.com/openpatch/bitflow/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "version": "pnpm build",
    "lint": "tsc --noEmit",
    "test": "jest --env=jsdom --passWithNoTests",
    "build": "pnpm build:locales && pnpm build:pkg && pnpm build:types",
    "build:pkg": "node ../../scripts/build.mjs",
    "build:locales": "vocab compile",
    "build:types": "tsc --project tsconfig.build.json --declaration --emitDeclarationOnly"
  },
  "dependencies": {
    "@bitflow/core": "workspace:*",
    "@bitflow/icons": "workspace:*",
    "@bitflow/provider": "workspace:*",
    "@bitflow/shell": "workspace:*",
    "zod": "3.17.3"
  },
  "peerDependencies": {
    "@emotion/cache": "11.x",
    "@emotion/react": "11.x",
    "@emotion/styled": "11.x",
    "@openpatch/patches": "6.x",
    "@vocab/core": "1.x",
    "@vocab/react": "1.x",
    "react": "18.x",
    "react-dom": "18.x",
    "react-hook-form": "7.x",
    "zod": "3.x"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@emotion/cache": "11.7.1",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@openpatch/patches": "6.0.1",
    "@types/react": "^18.0.12",
    "@types/react-dom": "18.0.5",
    "@vocab/core": "1.1.0",
    "@vocab/react": "1.1.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.3"
  }
}
