{
  "name": "@openpatch/bits-input-markdown",
  "version": "0.0.1",
  "author": "Mike Barkmin",
  "homepage": "https://github.com/openpatch/bits#readme",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openpatch/bits.git",
    "directory": "packages/input-markdown"
  },
  "bugs": {
    "url": "https://github.com/openpatch/bits/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "tsc --build --watch",
    "build": "concurrently yarn:build:*",
    "clean": "tsc --build --clean",
    "test": "jest --env=jsdom --passWithNoTests",
    "test:coverage": "yarn test --coverage",
    "version": "yarn build",
    "build:esm": "tsc --project tsconfig.json --declaration --declarationDir dist/esm --module esnext --target es2015 --outDir dist/esm",
    "build:cjs": "tsc --project tsconfig.json --declaration --declarationDir dist/cjs --module commonjs --target es5 --outDir dist/cjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types"
  },
  "dependencies": {
    "@openpatch/bits-base": "0.0.1"
  },
  "peerDependencies": {
    "@emotion/cache": "11.x",
    "@emotion/react": "11.x",
    "@emotion/styled": "11.x",
    "@openpatch/patches": "1.x",
    "react": "17.x",
    "react-dom": "17.x"
  }
}
